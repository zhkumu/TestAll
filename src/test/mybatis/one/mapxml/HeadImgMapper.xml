<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.mybatis.one.mapper.HeadImgMapper">
	<insert id="addImg" >
		insert into headimg(img)values(#{img})
	</insert>

	<select id="getImg" resultType="headImg">
		select * from headimg where id=1
	</select>
</mapper>